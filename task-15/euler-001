
#include <iostream>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while(t){
        int n,tot=0;
        cin >> n;
        
        while(n)
        {n--;
            if(n%5==0||n%3==0)
             tot=tot+n;
                   }
        cout<<tot<<"\n";
        t--;
    }
    return 0;
}
