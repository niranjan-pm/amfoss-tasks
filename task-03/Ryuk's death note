#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n,small;
      
    
    cin>>n;
    int a[n],b[n],c[n];
    
for(int i=0;i<n;i++)    
{cin>>a[i];
}
    for(int i=0;i<n;i++)
    {cin>>b[i];
    }

for(int i=0;i<n;i++)
{
   c[i]=b[i]/a[i] ;
}
    small=c[0];
        for(int j=0; j<n; j++)
        {
                if(small>c[j])
                {
                        small=c[j];
                }
        }
        cout<<small;

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
